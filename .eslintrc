{
    "extends": "eslint-config-airbnb",
  	"parser": "babel-eslint",
  	"env": {
      "browser": true,
      "node": true,
      "es6": true
  	},
  	"ecmaFeatures": {
      "jsx": true
  	},
  	"rules": {
      "indent": [
          2,
          "tab"
      ],
      "semi": [
          2,
          "always"
      ],
      "quotes": [
          2,
          "single"
      ],
  	}
}